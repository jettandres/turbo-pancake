{
  "name": "turbopancake",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native rnx-start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "check-dependencies": "rnx-dep-check",
    "bundle": "react-native rnx-bundle",
    "fix-dependencies": "rnx-dep-check --write"
  },
  "dependencies": {
    "jsc-android": "^250230.2.1",
    "react": "18.1.0",
    "react-native": "^0.70.0",
    "react-native-gradle-plugin": "^0.71.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.19.3",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/cli-platform-android": "^9.1.0",
    "@react-native-community/cli-platform-ios": "^9.1.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@rnx-kit/babel-preset-metro-react-native": "^1.1.1",
    "@rnx-kit/cli": "^0.14.8",
    "@rnx-kit/dep-check": "^1.13.0",
    "@rnx-kit/metro-config": "^1.3.1",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.21",
    "@testing-library/react-native": "^11.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/node": "^18.7.23",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^29.4.2",
    "detox": "^19.12.3",
    "detox-recorder": "^1.0.151",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "metro-config": "^0.72.1",
    "metro-react-native-babel-preset": "^0.72.1",
    "prettier": "2.7.1",
    "react-test-renderer": "18.1.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "rnx-kit": {
    "reactNativeVersion": "^0.70",
    "kitType": "app",
    "capabilities": [
      "babel-preset-react-native",
      "core",
      "core-android",
      "jest",
      "core-ios",
      "metro-config",
      "react",
      "react-test-renderer"
    ],
    "bundle": {
      "entryFile": "index.js",
      "targets": [
        "android",
        "ios"
      ],
      "platforms": {
        "android": {
          "bundleOutput": "android/app/src/main/assets/index.android.bundle",
          "sourcemapOutput": "android/main.jsbundle.map",
          "assetsDest": "android/app/src/main/res/"
        },
        "ios": {
          "bundleOutput": "ios/main.jsbundle",
          "sourcemapOutput": "ios/main.jsbundle.map",
          "assetsDest": "ios/rnx-assets"
        }
      },
      "treeShake": true
    }
  }
}
